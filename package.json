{
  "name": "react-project-example",
  "version": "1.0.0",
  "description": "Server side rendering template for React 16",
  "main": "src/server/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node src/server/index",
    "start:prod": "cross-env NODE_ENV=production node public/assets/server",
    "watch": "cross-env NODE_ENV=development babel-watch src/server/index",
    "build": "rimraf public/assets/* && npm run build:client && npm run build:server && npm run build:node",
    "build:client": "cross-env NODE_ENV=production webpack -p --config webpack/client.production.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config webpack/server.production.js",
    "build:node": "cross-env NODE_ENV=production webpack -p --config webpack/node.production.js"
  },
  "keywords": [
    "react",
    "fiber",
    "ejs",
    "ssr",
    "express",
    "server side rendering"
  ],
  "author": {
    "name": "Busygin Dmitriy",
    "email": ""
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cssnano": "^3.10.0",
    "ejs": "^2.5.7",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.0",
    "file-loader": "^1.1.11",
    "flush-css-chunks-webpack-plugin": "^0.2.0",
    "isomorphic-fetch": "^2.2.1",
    "multer": "^1.3.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-universal-component": "^2.5.5",
    "redux": "^3.7.2",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "serve-favicon": "^2.4.5",
    "webpack-flush-chunks": "^1.1.23",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.2.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.10",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^3.0.0",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-hot-server-middleware": "^0.3.2",
    "webpack-merge": "^4.1.1"
  }
}
